<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="questionDAO" class="io.disshell.DAO.QuestionDAOImpl">
        <constructor-arg ref="csvParser"></constructor-arg>
    </bean>
    <bean id="askQuestionService" class="io.disshell.service.AskQuestionServiceImpl">
        <constructor-arg ref="questionDAO"></constructor-arg>
    </bean>
    <bean id="csvParser" class="io.disshell.utils.CsvParserImpl"> </bean>

</beans>